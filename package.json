        "dbmr": "cross-env NODE_ENV=development prisma migrate reset -f",
        "dbmd": "cross-env NODE_ENV=production prisma migrate deploy",
        "dbs": "prisma db seed",
        "dbo": "prisma studio",
        "dball": "pnpm run dbm && pnpm run dbg && pnpm run dbs",
        "prepare": "husky",
        "lint-staged": {
